/* @import 'weui.wxss'; */

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.c::after {
    display: table;
    clear: both;
    content: '';
}

.w100 {
    width: 100%;
    display: block;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}
